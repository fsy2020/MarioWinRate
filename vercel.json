{
  "version": 2,
  "builds": [
    {
      "src": "server.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.py"
    }
  ],
  "functions": {
    "server.py": {
      "includeFiles": "data/**"
    }
  },
  "env": {
    "FLASK_ENV": "production"
  }
} 