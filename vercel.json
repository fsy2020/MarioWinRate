{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/server.py"
    }
  ],
  "functions": {
    "api/server.py": {
      "maxDuration": 30
    }
  },
  "env": {
    "PYTHONPATH": "."
  },
  "regions": ["iad1"]
} 